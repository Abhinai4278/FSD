public class TypeCasting {
public static void main(String[] args) {
	char c = 'A'; 
	System.out.println("Implicit Type Casting:");
	
	
	
	int i =c;
	System.out.println("Value of i :"+i);
	
	long l = c;
	
	System.out.println("Value of l :" +l);
	
	float f =c;
	
	System.out.println("Value of fl :" +f);
			
}
}
